<%= render partial: 'shared/errors', locals: {model: @list} %>

<br>

<%= form_for(@list) do |f| %>

  <%= f.hidden_field :created_by, value: current_user.name %>

  <%= f.label :name, "Task List Name:" %><br>
  <%= f.date_select :name, order: [:month, :day, :year] %><br>

  <br>

  <%= f.submit %>

<% end %>